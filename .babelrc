{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 5 versions",
                        "ie >= 8"
                    ]
                }
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-transform-react-jsx",
        "@babel/plugin-transform-arrow-functions",
        ["import", {
            "libraryName": "antd",
            "libraryDirectory": "es",
            "style": "css"
        }],


        ["import", {
            "libraryName": "tiklab-eam-ui",
            "libraryDirectory": "es",
            "style": true
        },"tiklab-eam-ui"],

        ["import", {
            "libraryName": "tiklab-privilege-ui",
            "libraryDirectory": "es",
            "style":  true
        },"tiklab-privilege-ui"],

        ["import", {
            "libraryName": "tiklab-user-ui",
            "libraryDirectory": "es",
            "style":  true
        },"tiklab-user-ui"],

        ["import", {
            "libraryName": "tiklab-licence-ui",
            "libraryDirectory": "es",
            "style":  true
        },"tiklab-licence-ui"],

        ["import", {
            "libraryName": "tiklab-message-ui",
            "libraryDirectory": "es",
            "style":  true
        },"tiklab-message-ui"],

        ["import", {
            "libraryName": "tiklab-plugin-ui",
            "libraryDirectory": "es",
            "style": true
        }, "tiklab-plugin-ui"],

        ["import", {
            "libraryName": "tiklab-oplog-ui",
            "libraryDirectory": "es",
            "style": true
        }, "tiklab-oplog-ui"],
        ["import", {
            "libraryName": "tiklab-todotask-ui",
            "libraryDirectory": "es",
            "style": true
        }, "tiklab-todotask-ui"],
        ["import", {
            "libraryName": "tiklab-widget-ui",
            "libraryDirectory": "es",
            "style":  true
        },"tiklab-widget-ui"],

        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["dynamic-import-webpack"],
        "@babel/plugin-syntax-dynamic-import",
        ["@babel/plugin-transform-runtime",
            {
                "regenerator": true
            }
        ],
        "@babel/plugin-transform-async-to-generator",
        "react-hot-loader/babel",
        ["@babel/plugin-syntax-jsx"]
    ]
}
